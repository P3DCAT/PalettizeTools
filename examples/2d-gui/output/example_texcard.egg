<Comment> {
  "egg-texture-cards -f rgba -o ../output/example_texcard.egg kart_Rim_1.png kart_Rim_2.png kart_Rim_3.png kart_Rim_4.png kart_Rim_5.png"
}
<Texture> kart_Rim_5 {
  kart_Rim_5.png
  <Scalar> format { rgba }
}
<Texture> kart_Rim_4 {
  kart_Rim_4.png
  <Scalar> format { rgba }
}
<Texture> kart_Rim_3 {
  kart_Rim_3.png
