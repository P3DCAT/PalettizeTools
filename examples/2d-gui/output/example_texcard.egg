      <VertexRef> { 0 1 2 3 <Ref> { vpool } }
    }
  }
  <Group> kart_Rim_4 {
    <Polygon> {
      <RGBA> { 1 1 1 1 }
      <TRef> { kart_Rim_4 }
      <VertexRef> { 0 1 2 3 <Ref> { vpool } }
    }
  }
  <Group> kart_Rim_5 {
    <Polygon> {
      <RGBA> { 1 1 1 1 }
      <TRef> { kart_Rim_5 }
      <VertexRef> { 0 1 2 3 <Ref> { vpool } }
    }
  }
